{
    "apiVersion": "7.1.3.0",
    "apis": [
        {
            "operations": [
                {
                    "method": "DELETE",
                    "nickname": "deleteAuthTokenManagementCommand",
                    "notes": "",
                    "parameters": [],
                    "summary": "Resets the Auth Token Management configuration to default values",
                    "type": "void"
                },
                {
                    "method": "GET",
                    "nickname": "getAuthTokenManagementCommand",
                    "notes": "",
