{
    "apiVersion": "7.1.3.0",
    "apis": [
        {
            "operations": [
                {
                    "method": "GET",
                    "nickname": "configExportCommand",
                    "notes": "",
                    "parameters": [],
                    "summary": "Export a JSON backup. This endpoint is not suitable for configurations that take longer than 30 seconds to export. For those configurations, use the \"/config/export/workflows\" endpoint instead.",
                    "type": "ExportData"
                }
            ],
            "path": "/config/export"
        },
        {
            "operations": [
                {
                    "method": "GET",
                    "nickname": "getConfigExportWorkflowsCommand",
                    "notes": "",
                    "parameters": [],
                    "summary": "Get the status of pending Exports",
                    "type": "ConfigStatusesView"
                },
                {
                    "method": "POST",
                    "nickname": "addConfigExportWorkflowCommand",
                    "notes": "",
                    "parameters": [],
                    "summary": "Start a JSON backup of the entire system for export",
                    "type": "ConfigStatusView"
                }
            ],
            "path": "/config/export/workflows"
        },
        {
            "operations": [
                {
                    "method": "GET",
                    "nickname": "getConfigExportWorkflowCommand",
                    "notes": "",
                    "parameters": [
                        {
                            "allowMultiple": false,
                            "description": "Id of Export to poll",
                            "name": "id",
                            "paramType": "path",
                            "required": true,
                            "type": "string"
                        }
                    ],
                    "summary": "Check the status of an Export",
                    "type": "ConfigStatusView"
                }
            ],
            "path": "/config/export/workflows/{id}"
        },
        {
            "operations": [
                {
                    "method": "GET",
                    "nickname": "getConfigExportWorkflowDataCommand",
                    "notes": "",
                    "parameters": [
                        {
                            "allowMultiple": false,
                            "description": "ID of Export to get",
                            "name": "id",
                            "paramType": "path",
                            "required": true,
                            "type": "string"
                        }
                    ],
                    "summary": "Export a JSON backup of the entire system",
                    "type": "ExportData"
                }
            ],
            "path": "/config/export/workflows/{id}/data"
        },
        {
            "operations": [
                {
                    "method": "POST",
                    "nickname": "configImportCommand",
                    "notes": "",
                    "parameters": [
                        {
                            "allowMultiple": false,
                            "description": "JSON backup file to import",
                            "name": "backup file",
                            "paramType": "body",
                            "required": true,
                            "type": "string"
                        }
                    ],
                    "summary": "Import a JSON backup. This endpoint is not suitable for configurations that take longer than 30 seconds to import. For those configurations, use the \"/config/import/workflows\" endpoint instead.",
                    "type": "void"
                }
            ],
            "path": "/config/import"
        },
        {
            "operations": [
                {
                    "method": "GET",
                    "nickname": "getConfigImportWorkflowsCommand",
                    "notes": "",
                    "parameters": [],
                    "summary": "Get the status of pending imports",
                    "type": "ConfigStatusesView"
                },
                {
                    "method": "POST",
                    "nickname": "addConfigImportWorkflowCommand",
                    "notes": "Start an Import of a JSON backup. If import fails for any reason, PingAccess will roll back to previous configuration.",
                    "parameters": [
                        {
                            "allowMultiple": false,
                            "description": "JSON backup file to import",
                            "name": "backup file",
                            "paramType": "body",
                            "required": true,
                            "type": "ExportData"
                        },
                        {
                            "allowMultiple": false,
                            "description": "When set to true, stops the import as soon as any validation errors are encountered. When false,  import will continue to validate configuration after the first failure to identify all validation errors. If any validation errors are present PingAccess will revert back to the state prior to import attempt.",
                            "name": "failFast",
                            "paramType": "query",
                            "required": true,
                            "type": "boolean"
                        }
                    ],
                    "summary": "Start an Import of a JSON backup.",
                    "type": "void"
                }
            ],
            "path": "/config/import/workflows"
        },
        {
            "operations": [
                {
                    "method": "GET",
                    "nickname": "getConfigImportWorkflowCommand",
                    "notes": "",
                    "parameters": [
                        {
                            "allowMultiple": false,
                            "description": "Id of Import to poll",
                            "name": "id",
                            "paramType": "path",
                            "required": true,
                            "type": "string"
                        }
                    ],
                    "summary": "Check the status of an Import",
                    "type": "ConfigStatusView"
                }
            ],
            "path": "/config/import/workflows/{id}"
        }
    ],
    "basePath": "https://localhost:9000/pa-admin-api/v3",
    "models": {
        "ApiErrorView": {
            "description": "An API error used by the PingAccess Administrative UI.",
            "id": "ApiErrorView",
            "properties": {
                "flash": {
                    "description": "The flash message displayed in the UI.",
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                },
                "form": {
                    "$ref": "Map[string,List[string]]",
                    "description": "The specific errors related to the current form."
                }
            },
            "required": [
                "flash",
                "form"
            ]
        },
        "ConfigStatusView": {
            "description": "An import or export configuration.",
            "id": "ConfigStatusView",
            "properties": {
                "apiErrorView": {
                    "$ref": "ApiErrorView",
                    "description": "The API error for import or export, if there is an error. When failFast=false this will collect all API errors encountered from the import configuration."
                },
                "currentEntity": {
                    "$ref": "JsonNode",
                    "description": "The current entity being imported or exported."
                },
                "id": {
                    "description": "The id of the configuration workflow.",
                    "format": "int32",
                    "type": "integer"
                },
                "status": {
                    "description": "The status of the configuration import or export.",
                    "type": "string"
                },
                "totalEntities": {
                    "description": "The total number of entities being imported or exported.",
                    "format": "int32",
                    "type": "integer"
                },
                "warnings": {
                    "$ref": "Set",
                    "description": "The API warnings for import or export, if there are any warnings.",
                    "items": {
                        "type": "string"
                    }
                }
            },
            "required": [
                "warnings"
            ]
        },
        "ConfigStatusesView": {
            "description": "A collection of import or export configuration workflows.",
            "id": "ConfigStatusesView",
            "properties": {
                "items": {
                    "description": "An array of import or export configuration workflows.",
                    "items": {
                        "$ref": "ConfigStatusView"
                    },
                    "type": "array"
                }
            },
            "required": [
                "items"
            ]
        },
        "ExportData": {
            "description": "A JSON backup file.",
            "id": "ExportData",
            "properties": {
                "data": {
                    "$ref": "ExportEntities",
                    "description": "The entities being exported."
                },
                "encryptionKey": {
                    "$ref": "JsonWebKey",
                    "description": "The randomly-generated export encryption key to be included with the data."
                },
                "masterKeys": {
                    "$ref": "MasterKeysView",
                    "description": "The encrypted master keys."
                },
                "version": {
                    "description": "The version of PingAccess that was exported.",
                    "type": "string"
                }
            },
            "required": [
                "data",
                "encryptionKey",
                "masterKeys",
                "version"
            ]
        },
        "JsonWebKey": {
            "description": "A JSON Web Key.",
            "id": "JsonWebKey",
            "properties": {
                "algorithm": {
                    "description": "The algorithm name.",
                    "type": "string"
                },
                "key": {
                    "$ref": "Key",
                    "description": "The key."
                },
                "keyId": {
                    "description": "The key id.",
                    "type": "string"
                },
                "keyOps": {
                    "description": "The list of key operations.",
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                },
                "keyType": {
                    "description": "The key type.",
                    "type": "string"
                },
                "publicKey": {
                    "$ref": "PublicKey",
                    "description": "The public key."
                },
                "use": {
                    "description": "The intended use of the key.",
                    "type": "string"
                }
            },
            "required": [
                "algorithm",
                "key",
                "keyId",
                "keyOps",
                "keyType",
                "publicKey",
                "use"
            ]
        },
        "Key": {
            "description": "A key.",
            "id": "Key",
            "properties": {
                "algorithm": {
                    "description": "The algorithm name for this key.",
                    "type": "string"
                },
                "encoded": {
                    "description": "The encoded key.",
                    "items": {
                        "type": "byte"
                    },
                    "type": "array"
                },
                "format": {
                    "description": "The encoding format.",
                    "type": "string"
                }
            },
            "required": [
                "algorithm",
                "encoded",
                "format"
            ]
        },
        "MasterKeysView": {
            "description": "An encrypted master key.",
            "id": "MasterKeysView",
            "properties": {
                "encryptedValue": {
                    "description": "The encrypted master key.",
                    "items": {
                        "type": "byte"
                    },
                    "type": "array"
                },
                "keyId": {
                    "description": "The key identifier.",
                    "type": "string"
                }
            },
            "required": [
                "encryptedValue",
                "keyId"
            ]
        }
    },
    "resourcePath": "/config",
    "swaggerVersion": "1.2"
}