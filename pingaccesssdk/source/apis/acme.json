    "models": {
        "AcmeAccountView": {
            "description": "An ACME Account associated with a CA.",
            "id": "AcmeAccountView",
            "properties": {
                "acmeServerId": {
                    "description": "The associated ACME Server.",
                    "type": "string"
                },
                "id": {
                    "description": "When creating a new AcmeAccount, this is the ID for the AcmeAccount. If not specified, an ID will be automatically assigned. When updating an existing AcmeAccount, this field is ignored and the ID is determined by the path parameter.",
                    "type": "string"
                },
                "keyAlgorithm": {
                    "description": "The key algorithm used to generate a key.",
                    "type": "string"
                },
                "privateKey": {
                    "$ref": "HiddenFieldView",
                    "description": "The account's private key data."
                },
                "publicKey": {
                    "$ref": "PublicKeyView",
                    "description": "The account's public key data."
                },
                "url": {
                    "description": "The URL the CA uses to reference the account.",
                    "type": "string"
                }
            }
        },
        "AcmeCertStatusView": {
            "description": "The status of a certificate.",
            "id": "AcmeCertStatusView",
            "properties": {
                "problems": {
                    "$ref": "Map[string,ProblemDocumentView]",
                    "description": "A map of problem documents for requested domains. The key is the domain and the value is a ProblemDocumentView."
                },
                "state": {
                    "$ref": "AcmeCertState",
                    "description": "The state of the certificate.",
                    "enum": [
                        "PENDING",
                        "READY",
                        "PROCESSING",
                        "VALID",
                        "INVALID",
                        "UNKNOWN"
                    ]
                }
            },
            "required": [
                "state"
            ]
        },
        "AcmeCertificateRequestView": {
            "description": "A reference to a Key Pair to be signed by the ACME protocol.",
            "id": "AcmeCertificateRequestView",
            "properties": {
                "acmeAccountId": {
                    "description": "The ID of the ACME Account to be used for the ACME protocol. This is read-only.",
                    "type": "string"
                },
                "acmeCertStatus": {
                    "$ref": "AcmeCertStatusView",
                    "description": "The status of the certificate."
                },
                "acmeServerId": {
                    "description": "The ID of the ACME Server to be used for the ACME protocol. This is read-only.",
                    "type": "string"
                },
                "id": {
                    "description": "When creating a new AcmeCertificateRequest, this is the ID for the AcmeCertificateRequest. If not specified, an ID will be automatically assigned. When updating an existing AcmeCertificateRequest, this field is ignored and the ID is determined by the path parameter.",
                    "type": "string"
                },
                "keyPairId": {
                    "description": "The ID of the Key Pair for which a signed certificate will be requested.",
                    "format": "int32",
                    "type": "integer"
                },
                "url": {
                    "description": "The URL at the ACME server for the associated ACME order.",
                    "type": "string"
                }
            },
            "required": [
                "keyPairId"
            ]
        },
        "AcmeServerView": {
            "description": "An ACME server.",
            "id": "AcmeServerView",
            "properties": {
                "acmeAccounts": {
                    "description": "An array of references to accounts. This array is read-only.",
                    "items": {
                        "$ref": "LinkView"
                    },
                    "type": "array"
                },
                "id": {
                    "description": "When creating a new AcmeServer, this is the ID for the AcmeServer. If not specified, an ID will be automatically assigned. When updating an existing AcmeServer, this field is ignored and the ID is determined by the path parameter.",
                    "type": "string"
                },
                "name": {
                    "description": "(sortable) A user-friendly name for the ACME server.",
                    "type": "string"
                },
                "url": {
                    "description": "The URL of the ACME directory resource on the ACME server.",
                    "type": "string"
                }
            },
            "required": [
                "name",
                "url"
            ]
