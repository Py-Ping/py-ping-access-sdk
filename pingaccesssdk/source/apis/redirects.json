        }
    ],
    "basePath": "https://localhost:9000/pa-admin-api/v3",
    "models": {
        "HostPortView": {
            "description": "A redirect source.",
            "id": "HostPortView",
            "properties": {
                "host": {
                    "description": "The hostname.",
                    "type": "string"
                },
                "port": {
                    "description": "The port number.",
                    "format": "int32",
                    "type": "integer"
                }
            },
            "required": [
                "host",
                "port"
            ]
        },
        "RedirectView": {
            "description": "A Redirect.",
            "id": "RedirectView",
            "properties": {
                "auditLevel": {
                    "$ref": "AuditLevel",
                    "description": "(sortable) Indicates if audit logging is enabled for the redirect.",
                    "enum": [
                        "ON",
                        "OFF"
                    ]
                },
                "id": {
                    "description": "When creating a new Redirect, this is the ID for the Redirect. If not specified, an ID will be automatically assigned. When updating an existing Redirect, this field is ignored and the ID is determined by the path parameter.",
                    "type": "string"
                },
                "responseCode": {
                    "description": "(sortable) The Redirect HTTP status code used by the response.",
                    "format": "int32",
                    "type": "integer"
                },
                "source": {
                    "$ref": "HostPortView",
                    "description": "(sortable) The source host and port of the Redirect. When used as a sort key, results are sorted by the host."
                },
                "target": {
                    "$ref": "TargetHostPortView",
                    "description": "(sortable) The target host and port of the Redirect. When used as a sort key, results are sorted by the host."
                }
            }
        },
        "RedirectsView": {
            "description": "A collection of Redirects.",
            "id": "RedirectsView",
            "properties": {
                "items": {
                    "description": "An array of Redirects.",
                    "items": {
                        "$ref": "RedirectView"
                    },
                    "type": "array"
                }
            },
            "required": [
                "items"
            ]
        },
        "TargetHostPortView": {
            "description": "A redirect target.",
            "id": "TargetHostPortView",
            "properties": {
                "host": {
                    "description": "The hostname.",
                    "type": "string"
                },
                "port": {
                    "description": "The port number.",
                    "format": "int32",
                    "type": "integer"
                },
                "secure": {
                    "description": "True if the target host expects HTTPS connections.",
                    "type": "boolean"
                }
            },
            "required": [
                "host",
                "port",
                "secure"
            ]
        }
    },
    "resourcePath": "/redirects",
    "swaggerVersion": "1.2"
}