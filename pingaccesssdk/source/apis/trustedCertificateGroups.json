    "basePath": "https://localhost:9000/pa-admin-api/v3",
    "models": {
        "RevocationCheckingView": {
            "description": "The client certificate revocation checking configuration.",
            "id": "RevocationCheckingView",
            "properties": {
                "crlChecking": {
                    "description": "This field is true if CRL client certificate revocation checking is enabled.",
                    "type": "boolean"
                },
                "denyRevocationStatusUnknown": {
                    "description": "This field is true if client certificates should be denied when the revocation status cannot be determined.",
                    "type": "boolean"
                },
                "ocsp": {
                    "description": "This field is true if OCSP client certificate revocation checking is enabled.",
                    "type": "boolean"
                },
                "skipTrustAnchors": {
                    "description": "When set to true, PA will skip validation of any certificates configured in the trusted certificate group as well as their subsequent chain of issuers when trusted certificates are found in the client certificate chain.",
                    "type": "boolean"
                },
                "supportDisorderedChain": {
                    "description": "When set to true, PA can validate client certificate chains that are not in the standard order of leaf -> intermediate(s) -> root. When false, validation may fail on client certificate chains that are not in the standard order.",
                    "type": "boolean"
                }
            }
        },
        "TrustedCertificateGroupView": {
            "description": "A trusted certificate group.",
            "id": "TrustedCertificateGroupView",
            "properties": {
                "certIds": {
                    "description": "The IDs of the certificates that are in the trusted certificate group.",
                    "items": {
                        "type": "int"
                    },
                    "type": "array"
                },
                "id": {
                    "description": "When creating a new TrustedCertificateGroup, this is the ID for the TrustedCertificateGroup. If not specified, an ID will be automatically assigned. When updating an existing TrustedCertificateGroup, this field is ignored and the ID is determined by the path parameter.",
                    "format": "int32",
                    "type": "integer"
                },
                "ignoreAllCertificateErrors": {
                    "description": "(sortable) This field is read-only and is only set to true for the Trust Any certificate group.",
                    "type": "boolean"
                },
                "name": {
                    "description": "(sortable) The name of the trusted certificate group.",
                    "type": "string"
                },
                "revocationChecking": {
                    "$ref": "RevocationCheckingView",
                    "description": "The configuration for client certificate revocation checking."
                },
                "skipCertificateDateCheck": {
                    "description": "(sortable) This field is true if certificates that have expired or are not yet valid but have passed the other certificate checks should be trusted.",
                    "type": "boolean"
                },
                "systemGroup": {
                    "description": "(sortable) This field is read-only and indicates the trusted certificate group cannot be modified.",
                    "type": "boolean"
                },
                "useJavaTrustStore": {
                    "description": "(sortable) This field is true if the certificates in the group should also include all certificates in the Java Trust Store.",
                    "type": "boolean"
                }
            },
            "required": [
                "name"
            ]
        },
        "TrustedCertificateGroupsView": {
            "description": "A collection of trusted certificate group items.",
            "id": "TrustedCertificateGroupsView",
            "properties": {
                "items": {
                    "description": "The actual list of trusted certificate groups.",
                    "items": {
                        "$ref": "TrustedCertificateGroupView"
                    },
                    "type": "array"
                }
            },
            "required": [
                "items"
            ]
        }
    },
    "resourcePath": "/trustedCertificateGroups",
    "swaggerVersion": "1.2"
}